{"ast":null,"code":"import ButtonComponent from \"../atom/ButtonComponent.vue\";\nimport FlexComponent from \"../atom/FlexComponent.vue\";\n// import Banner1 from \"\";\nexport default {\n  components: {\n    ButtonComponent,\n    FlexComponent\n  },\n  props: {\n    datas: {\n      type: Array,\n      default: []\n    }\n  },\n  name: \"BannerSlideComponent\",\n  data() {\n    return {\n      images: \"\",\n      position: 0,\n      index: 0,\n      items: null\n    };\n  },\n  computed: {\n    screen() {\n      return this.$store.state.screenSize;\n    }\n  },\n  mounted() {\n    this.items = document.querySelectorAll('.item-slider');\n    const screen = window.innerWidth;\n    this.$store.commit('setScreenSize', screen);\n    this.slideBanner();\n  },\n  methods: {\n    slideBanner() {\n      setInterval(() => this.nextSlide(), 3000);\n    },\n    nextSlide(event) {\n      if (this.items != undefined) {\n        if (this.index >= this.datas.length - 1) {\n          this.position = 0;\n          this.index = 0;\n        } else {\n          this.position += this.items[this.index].offsetWidth;\n          this.index++;\n        }\n      }\n    },\n    prevSlide(event) {\n      if (this.index > 0) {\n        this.position -= this.screen;\n        this.index--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["ButtonComponent","FlexComponent","components","props","datas","type","Array","default","name","data","images","position","index","items","computed","screen","$store","state","screenSize","mounted","document","querySelectorAll","window","innerWidth","commit","slideBanner","methods","setInterval","nextSlide","event","undefined","length","offsetWidth","prevSlide"],"sources":["D:\\MyProject\\e-comerce\\e_comerce2\\toko_ku\\ecomerce\\src\\components\\jumboTron\\BannerSlideComponent.vue"],"sourcesContent":["<template>\n  <div class=\"jumbotron w-full h-[210px] lg:h-[600px] bg-gray-400 overflow-hidden\">\n    <div class=\"wrapper-banner w-full h-full\">\n      <div class=\"slider-banner relative flex items-center h-full w-full\">\n        <i class=\"bx bx-chevron-left absolute left-2 z-30 text-[2em]\" @click=\"prevSlide\"></i>\n        <i class=\"bx bx-chevron-right absolute right-2 z-30 text-[2em]\" @click=\"nextSlide\"></i>\n        <div class=\"absolute z-30 bottom-0 h-max flex gap-2 justify-center items-center py-2 w-full\">\n          <span v-for=\"(banner, i) in datas\" :key=\"i\" :class=\"index == i ? 'bg-red-400' : 'bg-slate-50'\" class=\"rounded-full w-[8px] h-[8px] block\"></span>\n        </div>\n        <div class=\"content-slide min-w-full flex flex-nowrap h-full transition-all duration-200\" :style=\"`transform: translateX(-${position}px)`\">\n          <FlexComponent v-for=\"(banner, i) in datas\" :key=\"i\" :addClass=\"'min-w-full item-slider relative lg:px-40 h-full'\" :align=\"'center'\">\n            <img\n              :src=\"banner.image\"\n              class=\"z-10 w-full h-full lg:top-0 lg:bottom-0 lg:left-0 lg:right-0 absolute\"\n              alt=\"\"\n            />\n            <div class=\"text-banner min-w-[80%] px-4 lg:px-0 relative z-30 lg:min-w-[50%] max-w-[50%] h-max\">\n              <h1 class=\"font-bold lg:text-6xl text-md mb-2 text-slate-50 lg:mb-4\">\n                {{ banner.title }}\n              </h1>\n              <p\n                class=\"lg:font-semibold font-light text-xs mb-4 lg:text-base lg:mb-14 text-slate-50 text-justify tracking-wide\"\n              >\n                {{ banner.subTitle }}\n              </p>\n              <ButtonComponent\n                text=\"Shop Now\"\n                :customClass=\"'bg-slate-50'\"\n                :colorText=\"'text-slate-900'\"\n                :fontSize=\"screen <= 540 ? '0.6em' : '1em'\"\n                fontWeight=\"semibold\"\n                rounded=\"rounded\"\n              />\n            </div>\n          </FlexComponent>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport ButtonComponent from \"../atom/ButtonComponent.vue\";\nimport FlexComponent from \"../atom/FlexComponent.vue\";\n// import Banner1 from \"\";\nexport default {\n  components: {\n    ButtonComponent,\n    FlexComponent,\n  },\n  props: {\n    datas: {\n      type: Array,\n      default: []\n    }\n  },\n  name: \"BannerSlideComponent\",\n  data() {\n    return {\n      images: \"\",\n      position: 0,\n      index: 0,\n      items: null,\n    };\n  },\n  computed: {\n    screen() {\n      return this.$store.state.screenSize;\n    }\n  },\n  mounted() {\n    this.items = document.querySelectorAll('.item-slider');\n    const screen = window.innerWidth;\n    this.$store.commit('setScreenSize', screen);\n    this.slideBanner()\n  },\n  methods: {\n    slideBanner() {\n      setInterval(() => this.nextSlide(), 3000)\n    },\n    nextSlide(event) {\n      if(this.items != undefined) {\n        if(this.index >= (this.datas.length - 1)) {\n          this.position = 0;\n          this.index = 0;\n        }else {\n          this.position += this.items[this.index].offsetWidth;\n          this.index++;\n        }\n      }\n    },\n    prevSlide(event) {\n      if(this.index > 0) {\n        this.position -= this.screen;\n        this.index--;\n      }\n    }\n  }\n};\n</script>\n"],"mappings":"AAyCA,OAAOA,eAAc,MAAO,6BAA6B;AACzD,OAAOC,aAAY,MAAO,2BAA2B;AACrD;AACA,eAAe;EACbC,UAAU,EAAE;IACVF,eAAe;IACfC;EACF,CAAC;EACDE,KAAK,EAAE;IACLC,KAAK,EAAE;MACLC,IAAI,EAAEC,KAAK;MACXC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,IAAI,EAAE,sBAAsB;EAC5BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,CAAC;MACXC,KAAK,EAAE,CAAC;MACRC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,MAAMA,CAAA,EAAG;MACP,OAAO,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,UAAU;IACrC;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACN,KAAI,GAAIO,QAAQ,CAACC,gBAAgB,CAAC,cAAc,CAAC;IACtD,MAAMN,MAAK,GAAIO,MAAM,CAACC,UAAU;IAChC,IAAI,CAACP,MAAM,CAACQ,MAAM,CAAC,eAAe,EAAET,MAAM,CAAC;IAC3C,IAAI,CAACU,WAAW,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,WAAWA,CAAA,EAA